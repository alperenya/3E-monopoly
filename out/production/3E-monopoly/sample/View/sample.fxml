      <Button layoutX="325.0" layoutY="313.0" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#settingsButtonPushed" prefHeight="50.0" prefWidth="200.0" styleClass="entrance-button" text="Settings" textFill="WHITE" />

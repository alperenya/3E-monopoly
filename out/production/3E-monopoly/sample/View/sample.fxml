      <Button layoutX="325.0" layoutY="471.0" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#creditsButtonPushed" prefHeight="50.0" prefWidth="200.0" styleClass="entrance-button" text="Credits" textFill="WHITE" />
      <Button fx:id="closeButton" layoutX="325.0" layoutY="546.0" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#closeButtonAction" prefHeight="50.0" prefWidth="200.0" styleClass="entrance-button" text="Exit" textFill="WHITE" />
